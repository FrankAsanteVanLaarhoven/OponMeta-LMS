$('.dropinscript').hide();
var submittingBoxer=false;
$('.boxer-register-form').submit(function (event) {
  //event.preventDefault();
  //event.stopPropagation();
  
  if (submittingBoxer) {
    return;
  }
  
  var email = $(this).find('.emailboxer').val();
