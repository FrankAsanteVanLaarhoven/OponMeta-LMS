      trackSignupButtonClick(text);
      
    });

    function trackSignupButtonClick(buttonText) {
      posthog.capture('signup_button_clicked', {
        button_text: buttonText,
        clicked_at: new Date().toISOString(),
        page_url: window.location.href,
      });
    }
  });
</script>



