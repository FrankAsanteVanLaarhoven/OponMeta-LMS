      /[A-Z]/,
      /[a-z]/,
      /[0-9]/,
      /[!@#\$%\^&\*\(\)_\+\-=\[\]\{\};':"\\|,.<>\/?]/
    ];
    var passwordChecks = passwordRegexes.map(function (regex) {
