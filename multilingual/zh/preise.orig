// Get all input fields with the class "subdomain"
var inputFields = document.getElementsByClassName("subdomain");
// Iterate through each input field
for (var i = 0; i < inputFields.length; i++) {
    // Add event listener for input to each input field
    inputFields[i].addEventListener("input", function (event) {
        // Get the current input value
        var inputValue = event.target.value;
        // Define the regular expression pattern to allow only letters, numbers, "-", and "#"
        var pattern = /^[a-zA-Z0-9\-#]*$/;
        // Check if the input value matches the pattern
        if (!pattern.test(inputValue)) {
            // If the input value does not match the pattern, remove invalid characters
            event.target.value = inputValue.replace(/[^a-zA-Z0-9\-]/g, "");
        }
    });
}


