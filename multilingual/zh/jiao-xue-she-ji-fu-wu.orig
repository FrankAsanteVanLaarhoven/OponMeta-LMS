    },
    data: JSON.stringify({ "data": { "user": { "firstName": firstName, inIframe: true, "lastName": lastName, "email": email, "password": password, "status": ["BOXER"] }, "offerCode": "" } })
  };

  $.ajax(settings).done(function (response) {
