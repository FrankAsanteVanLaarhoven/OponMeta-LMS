      const el = e.target.closest('a.button-primary, input.button-primary');
      if (!el) return;

      const text = el.tagName === 'INPUT' ? el.value.trim() : el.innerText.trim();

      console.log('✅ Tracked click:', text);
     
      trackSignupButtonClick(text);
      
