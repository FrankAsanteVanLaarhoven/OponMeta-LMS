function formatPlans(data) {

  if(!data){
    var currency = "usd";
    var currencySymbol = "$";
    plans = defaultPlans(currency,currencySymbol);
  }else{
    var currency = data.expert.monthly.currency;
    var currencySymbol = data.expert.monthly.currencySymbol;
    plans = data;
  }

  selected_currency = currency;

  plans["enterprise"] = {
    "monthly": {
      "price": enterprisePrices[currency].monthly,
      "period": "month",
      "currency": currency,
      "currencySymbol": currencySymbol
    },
    "yearly": {
      "price": enterprisePrices[currency].yearly,
      "period": "year",
      "currency": currency,
      "currencySymbol": currencySymbol
    }
  };
  plans["addons"] = {
    "monthly": {
      "price": addonsPrices[currency].monthly,
      "period": "month",
      "currency": currency,
      "currencySymbol": currencySymbol
    },
    "yearly": {
      "price": addonsPrices[currency].yearly,
      "period": "year",
      "currency": currency,
      "currencySymbol": currencySymbol
    }
  };

  $('.prices-currency').html(currency);

  $('.prices-list-free').html('<div class="card-plan-item-price">' + currencySymbol + '0 <span class="user_currency">' + currency.toUpperCase() + '</span></div>');
  $('.pricing-table-free').html(currencySymbol + '0 <span class="user_currency">' + currency.toUpperCase() + '</span>');

  Object.entries(plans).forEach(function ([level, level_plans]) {
      Object.entries(level_plans).forEach(function ([periodicity, plan]) {
        if(plan.c_code){
          $(".promotion_badge").show();
        }
        base_price = "";
        if (periodicity === "monthly") {
          decimals = (plan.price % 1).toFixed(2).substring(2);
          if (decimals == 0) decimals = "";
          if(plan.c_code){
            base_price = "<span style='font-size: 20px;text-decoration: line-through;'>" + plan.currencySymbol + plan.base_price.toLocaleString('en-US') + "</span> ";
          }
          $('.prices-list-'+level+'-'+periodicity).html('<div class="card-plan-item-price">' + base_price + plan.currencySymbol + Math.trunc(plan.price).toLocaleString('en-US') + '<sup class="superscript">' + decimals + '</sup> <span class="user_currency">' + currency.toUpperCase() + '</span></div>');
          $('.pricing-table-'+level+'-'+periodicity).html(plan.currencySymbol + plan.price.toLocaleString('en-US') + ' <span class="user_currency">' + currency.toUpperCase() + '</span>');
        } else {
          if(plan.c_code){
            base_price = "<span style='font-size: 20px;text-decoration: line-through;'>" + plan.currencySymbol + Math.ceil(plan.base_price / 12).toLocaleString('en-US') + "/m</span> ";
          }
          var billed_yearly = $('.prices-list-'+level+'-'+periodicity).find('.text-span div').html();
            $('.prices-list-'+level+'-'+periodicity).html('<div class="card-plan-item-price">' + base_price + plan.currencySymbol + Math.ceil(plan.price / 12).toLocaleString('en-US') + '/m <span class="user_currency">' + currency.toUpperCase() + '</span><div class="text-span">' + plan.currencySymbol + plan.price.toLocaleString('en-US') + ' ' + billed_yearly + '</div></div>');
            $('.pricing-table-'+level+'-'+periodicity).html(plan.currencySymbol + Math.ceil(plan.price / 12).toLocaleString('en-US') + '/m <span class="user_currency">' + currency.toUpperCase() + '</span><div class="text-span">' + plan.currencySymbol + plan.price.toLocaleString('en-US') + ' ' + billed_yearly + '</div>');
        }
      });
    });
}

function getPlansForIp(ip){
  var url = "https://my.coursebox.ai/api/ChargebeeSubscriptions/getPlansForIp?ip=" + ip;

  fetch(url)
    .then(function(response) {
    // Process the response here
    return response.json(); // or response.text(), etc.
  })
    .then(function(data) {
    // Use the data received from the fetch call
    formatPlans(data);
    c_code = data.expert.monthly.c_code;
  })
    .catch(function(error) {
    // Handle any errors that occurred during the fetch
    console.error('Error:', error);
    formatPlans();
  });
}


function geolocateUser() {

  var url = "https://api.ipify.org?format=json";
  const params = {
    redirect: "follow",
    referrer: "no-referrer"
  };

  fetch(url, params)
    .then(function(response) {
    // Process the response here
    return response.json(); // or response.text(), etc.
  })
    .then(function(data) {
    // Use the data received from the fetch call
    getPlansForIp(data.ip)
  })
    .catch(function(error) {
    // Handle any errors that occurred during the fetch
    console.error('Error:', error);
    getPlansForIp();
  });

}

